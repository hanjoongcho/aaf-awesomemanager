    <!--미사용 스타일 정리해야 함-->
    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.Light.DarkActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/colorAccent</item>
        <item name="android:windowBackground">@color/white</item>
        <!--<item name="android:dropDownListViewStyle">@style/customDropDownListViewStyle</item>-->
        <!-- <item name="android:spinnerItemStyle">@style/SpinnerItem</item>
         <item name="android:dropDownListViewStyle">@style/SpinnerDropDownItem</item>-->
        <item name="customTextViewStyle">@style/Widget.TextView.AppTheme</item>
        <!--<item name="android:buttonStyle">@style/ButtonAppTheme</item>
        <item name="android:imageButtonStyle">@style/ImageButtonAppTheme</item>-->
    </style>
    <style name="SpinnerItem">
        <item name="android:textColor">#993399</item>
        <item name="android:background">@drawable/spinner_simple</item>
    </style>
    <style name="SpinnerDropDownItem">
        <item name="android:textColor">#993399</item>
        <item name="android:background">@drawable/spinner_simple</item>
    </style>
    <style name="customDropDownListViewStyle" parent="android:Widget.ListView.DropDown">
        <item name="android:divider">@color/colorPrimary</item>
        <item name="android:dividerHeight">1dp</item>
        <item name="android:background">@color/white</item>
    </style>
    <style name="AppTheme.NoActionBar">
        <item name="windowActionBar">false</item>
        <item name="windowNoTitle">true</item>
    </style>
    <style name="AppTheme.AppBarOverlay" parent="ThemeOverlay.AppCompat.Dark.ActionBar" />
    <style name="AppTheme.PopupOverlay" parent="ThemeOverlay.AppCompat.Light" />

    <!--Custom Theme-->
    <style name="CustomPurple" parent="Theme.AppCompat.Light.DarkActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/blueColorPrimary</item>
        <item name="colorPrimaryDark">@color/blueColorPrimaryDark</item>
        <item name="colorAccent">@color/blueColorAccent</item>
        <item name="android:windowBackground">@color/white</item>
        <item name="android:dropDownListViewStyle">@style/customDropDownListViewStyle</item>
        <item name="windowActionBar">false</item>
        <item name="windowNoTitle">true</item>
        <item name="customTextViewStyle">@style/Widget.TextView.CustomPurple</item>
    </style>
    <style name="CustomPurple.NoActionBar">
        <item name="windowActionBar">false</item>
        <item name="windowNoTitle">true</item>
    </style>
    <style name="CustomPurple.AppBarOverlay" parent="ThemeOverlay.AppCompat.Dark.ActionBar" />
    <style name="CustomPurple.PopupOverlay" parent="ThemeOverlay.AppCompat.Light" />

    <style name="CustomPurpleWithActionBar" parent="Theme.AppCompat.Light.DarkActionBar">
        <item name="colorPrimary">@color/blueColorPrimary</item>
        <item name="colorPrimaryDark">@color/blueColorPrimaryDark</item>
        <item name="colorAccent">@color/blueColorAccent</item>
        <item name="android:windowBackground">@color/white</item>
        <item name="android:dropDownListViewStyle">@style/customDropDownListViewStyle</item>
        <item name="customTextViewStyle">@style/Widget.TextView.CustomPurple</item>
    </style>
